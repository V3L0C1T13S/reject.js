    "dependencies": {
        "discord.js": "^13.7.0",
        "revolt.js": "^6.0.0-2"
    },
    "devDependencies": {
        "@types/node": "^17.0.34",
        "rimraf": "^3.0.2",
        "typescript": "^4.6.4"
