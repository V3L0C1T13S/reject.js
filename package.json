{
    "type": "module",
    "name": "reject.js",
    "version": "1.0.0",
    "description": "Converts Discord.js API calls into Revolt.js API calls",
    "author": "V3L0C1T13S",
    "main": "dist/index.js",
    "files": ["dist/**/*"],
    "license": "MIT",
    "keywords": ["revolt.js", "revolt.chat", "revolt", "compatibility"],
    "scripts": {
